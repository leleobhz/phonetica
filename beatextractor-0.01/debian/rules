#!/usr/bin/make -f
# -*- makefile -*-
# Sample debian/rules that uses debhelper.
# This file was originally written by Joey Hess and Craig Small.
# As a special exception, when this file is copied by dh-make into a
# dh-make output file, you may use that output file without restriction.
# This special exception was added by Craig Small in version 0.37 of dh-make.

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

PACKAGE := beatextractor
TOPDIR := $(shell pwd)
t = ${TOPDIR}/debian/${PACKAGE}

include /usr/share/dpatch/dpatch.make

build: build-stamp patch

build-stamp:  
	touch $@
clean: unpatch
	dh_testdir
	dh_testroot
	rm -f build-stamp install-stamp
	dh_clean

install: build
	dh_testdir
	dh_testroot
	dh_clean -k 
	dh_installdirs

	# Add here commands to install the package into debian/beatextractor.
	cp -p $(CURDIR)/BeatExtractor.kmdr $(CURDIR)/BeatExtractorRecursive.sh $(CURDIR)/BeatExtractor.sh $(CURDIR)/GraphicLauncher.sh $(CURDIR)/Progress.sh $(CURDIR)/README $(CURDIR)/target.btex $(CURDIR)/debian/beatextractor/usr/share/BeatExtractor/


# Build architecture-independent files here.
binary-indep: build install
# We have nothing to do by default.

# Build architecture-dependent files here.
binary-arch: build install
	dh_testdir
	dh_testroot
	dh_installchangelogs 
	dh_installdocs
	dh_installexamples
#	dh_install
#	dh_installmenu
#	dh_installdebconf	
#	dh_installlogrotate
#	dh_installemacsen
#	dh_installpam
#	dh_installmime
#	dh_python
#	dh_installinit
#	dh_installcron
#	dh_installinfo
	dh_installman debian/bbep.1
	dh_link
	dh_strip
	dh_compress
	dh_fixperms
#	dh_perl
#	dh_makeshlibs
	dh_installdeb
	dh_shlibdeps
	dh_gencontrol
	dh_md5sums
	dh_builddeb

binary: binary-indep binary-arch
.PHONY: build clean binary-indep binary-arch binary install configure
